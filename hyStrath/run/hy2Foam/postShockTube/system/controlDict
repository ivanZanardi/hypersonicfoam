FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     hy2Foam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         2.5e-05;

deltaT          1e-10;

writeControl    adjustableRunTime;

writeInterval   5e-6;

cycleWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   10;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           0.2;

maxDeltaT       1e-4;

